{
  "name": "runbook-admin",
  "version": "0.1.0",
  "description": "Local GUI for running and managing Runn runbooks",
  "type": "module",
  "bin": {
    "runbook-admin": "./dist/bin/runbook-admin.js"
  },
  "scripts": {
    "dev": "vite --config vite.config.ts",
    "build": "vite build && npm run build:cli",
    "build:cli": "tsc src/bin/runbook-admin.ts --outDir dist/bin --target es2022 --module node16 --moduleResolution node16",
    "preview": "vite preview",
    "start": "node dist/bin/runbook-admin.js"
  },
  "dependencies": {
    "hono": "^4.6.0",
    "honox": "^0.1.24",
    "@hono/node-server": "^1.12.0",
    "chokidar": "^3.5.3",
    "fast-glob": "^3.3.2",
    "js-yaml": "^4.1.0",
    "open": "^10.1.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.5.0",
    "@types/ws": "^8.5.12",
    "typescript": "^5.6.0",
    "vite": "^5.4.0"
  },
  "keywords": [
    "runn",
    "runbook",
    "testing",
    "automation",
    "cli",
    "gui"
  ],
  "author": "runbook-admin",
  "license": "MIT"
}